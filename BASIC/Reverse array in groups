 Reverse array in groups 
 //https://practice.geeksforgeeks.org/problems/reverse-array-in-groups0255/1#
 
 
 
 void reverseInGroups(vector<long long>& a, int n, int k){
        // code here
        
        for(int i=0;i<n;i+=k)
        {
            int l=i;
           int  r=min(i+k-1,n-1);
            while(l<r)
            swap(a[l++],a[r--]);
        }
        
        
        
        }
    
        
