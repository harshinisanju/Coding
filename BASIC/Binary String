 Binary String
 
 //https://practice.geeksforgeeks.org/problems/binary-string-1587115620/1#
 
 long binarySubstring(int n, string s){
        
        // Your code here
        int flag=0;
        int c=0;
        
        for(int i=0;i<n;i++)
        {
        if(s[i]=='1')
        c++;
        }
        c=c*(c-1)/2;
       
        return c;
    }

};
